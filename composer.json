{
    "name": "hgabka/media-bundle",
    "type": "Symfony bundle",
    "description": "Media bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/hgabka/media-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "hgabka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=8.0.2",
        "hgabka/utils-bundle": "^6.0"
        "liip/imagine-bundle": "^2.1",
        "knplabs/gaufrette": "^0.10",
        "symfony/framework-bundle": "6.0.*",
        "twig/intl-extra": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Hgabka\\MediaBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hgabka\\MediaBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
